package demo;

class Callme{
	void call(String msg) {
		System.out.println("[" + msg);
		try {
			Thread.sleep(1000);
		}catch(InterruptedException e) {
			System.out.println("Interrupted");
		}
		System.out.println("]");
	}
}

class Caller implements Runnable{
	String msg;
	Callme target;
	Thread t;
}


public class Synchronize {
	
}
